@article{levinUnderstandingResponsePoly2024,
  title = {Understanding the {{Response}} of {{Poly}}(Ethylene Glycol) Diacrylate ({{PEGDA}}) {{Hydrogel Networks}}: {{A Statistical Mechanics-Based Framework}}},
  shorttitle = {Understanding the {{Response}} of {{Poly}}(Ethylene Glycol) Diacrylate ({{PEGDA}}) {{Hydrogel Networks}}},
  author = {Levin, Michal and Tang, Yongkui and Eisenbach, Claus D. and Valentine, Megan T. and Cohen, Noy},
  date = {2024-07-17},
  journaltitle = {Macromolecules},
  shortjournal = {Macromolecules},
