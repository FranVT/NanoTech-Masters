  file = {C\:\\Users\\miniFran\\Zotero\\storage\\6BJBC7B2\\Ruiz-Franco et al. - 2023 - Concentration and temperature dependent interactio.pdf;C\:\\Users\\miniFran\\Zotero\\storage\\UTYDM5UR\\Ruiz-Franco et al. - 2023 - Concentration and temperature dependent interactio.pdf}
