@article{duttaBridgingSimulationLength2019,
  title = {Bridging {{Simulation Length Scales}} of {{Bottlebrush Polymers Using}} a {{Wormlike Cylinder Model}}},
  author = {Dutta, Sarit and Pan, Tianyuan and Sing, Charles E.},
  date = {2019-07-09},
  journaltitle = {Macromolecules},
