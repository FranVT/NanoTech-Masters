@article{hazraStructureResponsiveMicrogels2024,
  title = {Structure of {{Responsive Microgels}} down to {{Ultralow Cross-Linkings}}},
  author = {Hazra, Nabanita and Ninarello, Andrea and Scotti, Andrea and Houston, Judith E. and Mota-Santiago, Pablo and Zaccarelli, Emanuela and Crassous, Jérôme J.},
  date = {2024-01-09},
