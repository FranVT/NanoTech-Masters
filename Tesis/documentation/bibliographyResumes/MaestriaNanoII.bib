}

@article{chatziminaStabilityAnnularPoiseuille2009,
  title = {Stability of the Annular {{Poiseuille}} Flow of a {{Newtonian}} Liquid with Slip along the Walls},
  author = {Chatzimina, Maria and Georgiou, Georgios C. and Housiadas, Kostas and Hatzikiriakos, Savvas G.},
