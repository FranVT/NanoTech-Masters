@article{rousselMarshConeTest2005b,
  title = {The {{Marsh}} Cone: A Test or a Rheological Apparatus?},
  shorttitle = {The {{Marsh}} Cone},
  author = {Roussel, Nicolas and Le Roy, Robert},
  year = {2005},
